spring:
  application:
    name: CHERRY

server:
  port: ${PORT}
  
instance:
  index: ${INSTANCE_INDEX}
  
eureka:
  instance:
    hostname: ${vcap.application.uris[0]}
    prefer-ip-address: true
    ip-address: ${CF_INSTANCE_INTERNAL_IP}
    non-secure-port: ${PORT}
  client:
    serviceUrl:
      defaultZone: ${vcap.services.eureka-server.credentials.uri}

  